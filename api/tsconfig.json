{
  "compilerOptions": {
    "target": "ES2017",
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "noImplicitThis": true,
    "alwaysStrict": true
  },
  "exclude": [
    "../node_modules"
  ]
}